{
  "name": "monitoring-system-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "report-worker:start:dev": "dotenv -e ./config/env/.env.development -- cross-env NODE_ENV=development nest start --watch --config nest-cli-report-worker.json",
    "data-raw-worker:start:dev": "dotenv -e ./config/env/.env.development -- cross-env NODE_ENV=development nest start --watch --config nest-cli-data-raw-worker.json",
    "data-raw-cron:start:dev": "dotenv -e ./config/env/.env.development -- cross-env NODE_ENV=development nest start --watch --config nest-cli-data-raw-cron.json",
    "start:dev": "dotenv -e ./config/env/.env.development -- cross-env NODE_ENV=development nest start --watch",
    "start:debug": "dotenv -e ./config/env/.env.development -- cross-env NODE_ENV=development nest start --debug --watch",
    "report-worker:start:prod": "dotenv -e config/env/.env.production -- cross-env NODE_ENV=production node dist/src/workers/report-worker/report-worker",
    "data-raw-worker:start:prod": "dotenv -e config/env/.env.production -- cross-env NODE_ENV=production node dist/src/workers/data-raw-worker/data-raw-worker",
    "data-raw-cron:start:prod": "dotenv -e config/env/.env.production -- cross-env NODE_ENV=production node dist/src/cron/raw-data-cron/data-raw-cron",
    "start:prod": "dotenv -e config/env/.env.production-- cross-env NODE_ENV=production node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "generate-schema": "cat src/*/*.prisma > prisma/schema.prisma",
    "migrate:dev": "dotenv -e ./config/env/.env.development -- npx prisma migrate dev --preview-feature --name init && prisma generate",
    "migrate:prod": "dotenv -e config/env/.env.production -- npx prisma migrate dev --preview-feature --name init && prisma generate",
    "seed:dev": "dotenv -e ./config/env/.env.development -- npx prisma db seed",
    "seed:prod": "dotenv -e config/env/.env.production -- npx prisma db seed",
    "studio:dev": "dotenv -e ./config/env/.env.development -- npx prisma studio",
    "start:infra": ""
  },
  "dependencies": {
    "@casl/ability": "^6.7.1",
    "@casl/prisma": "^1.4.1",
    "@nestjs/axios": "^3.0.2",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.4.18",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/websockets": "^10.4.18",
    "@prisma/client": "^5.12.1",
    "@sindresorhus/slugify": "^2.2.1",
    "@types/aws-sdk": "^2.7.0",
    "@types/passport-local": "^1.0.38",
    "@types/uuid": "^9.0.8",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "aws-sdk": "^2.1625.0",
    "bcrypt": "^5.1.1",
    "bullmq": "^5.10.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "convert-pino-request-to-curl": "^0.0.29",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv-cli": "^7.3.0",
    "firebase-admin": "^13.4.0",
    "ioredis": "^5.4.1",
    "luxon": "^3.4.4",
    "module-alias": "^2.2.3",
    "moment": "^2.30.1",
    "nestjs-config": "^1.4.11",
    "nestjs-http-promise": "^3.0.0",
    "nestjs-pino": "^4.0.0",
    "otp-generator": "^4.0.1",
    "passport": "^0.7.0",
    "passport-headerapikey": "^1.2.2",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.17.2",
    "pino-http": "^9.0.0",
    "pino-pretty": "^10.3.1",
    "prom-client": "^15.1.3",
    "randomstring": "^1.3.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "slugify": "^1.6.6",
    "supertokens-node": "^16.7.3",
    "uuid": "^10.0.0",
    "xlsx": "^0.18.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/ms": "^0.7.34",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/randomstring": "^1.3.0",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.12.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
